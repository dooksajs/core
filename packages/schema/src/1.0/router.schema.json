{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schema.dooksa.org/1.0/router.schema.json",
  "title": "Router",
  "$defs": {
    "router_currentId": {
      "title": "Hash current pathname",
      "const": "$null"
    },
    "router_currentPath": {
      "title": "Current pathname",
      "const": "$null"
    },
    "router_navigate": {
      "type": "object",
      "properties": {
        "to": {
          "$ref": "./base.schema.json#/$defs/string"
        }
      }
    }
  }
}