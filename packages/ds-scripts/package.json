{
  "name": "@dooksa/ds-scripts",
  "version": "0.0.0",
  "private": true,
  "author": "Thomas David <tom@mightysmile.com>",
  "repository": "git@github.com:dooksajs/dooksa.git",
  "type": "module",
  "bin": {
    "ds-scripts": "./bin/ds-scripts.js"
  },
  "imports": {
    "#development": "../../../app/development/*",
    "#production": "../../../app/production/*"
  },
  "env": {
    "test:run": {
      "plugins": [
        "istanbul"
      ]
    },
    "test:open": {
      "plugins": [
        "istanbul"
      ]
    }
  },
  "standard": {
    "env": [
      "browser",
      "mocha"
    ],
    "globals": [
      "cy",
      "Cypress",
      "expect",
      "assert"
    ]
  },
  "dependencies": {
    "@dooksa/crypto": "workspace:^",
    "@dooksa/ds-app-client": "workspace:^",
    "@dooksa/ds-app-server": "workspace:^",
    "@dooksa/ds-server-plugin-development": "workspace:^",
    "@dooksa/parse-action": "workspace:^",
    "@dooksa/parse-template": "workspace:^",
    "chalk": "^5.3.0",
    "cypress": "^11.2.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.19.5",
    "get-user-locale": "^2.3.1",
    "standard": "^17.1.0"
  }
}
