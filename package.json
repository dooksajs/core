{
  "name": "@dooksa/resource-loader",
  "version": "1.0.0",
  "description": "Resource loader provides a convenient wrapper to add scripts or link ot the DOM",
  "main": "src/index.js",
  "scripts": {
    "lint": "standard",
    "format": "standard --fix",
    "dev": "ds-scripts dev",
    "build": "ds-scripts build",
    "test:unit": "ds-scripts --unit test",
    "test:unit:record": "ds-scripts --unit --record test",
    "test:e2e": "ds-scripts --e2e test",
    "test:e2e:record": "ds-scripts --e2e --record test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/dooksa/resource-loader.git"
  },
  "author": {
    "name": "Thomas David",
    "email": "tom@mightysmile.com"
  },
  "homepage": "https://bitbucket.org/dooksa/resource-loader#readme",
  "devDependencies": {
    "@dooksa/ds-scripts": "bitbucket:dooksa/ds-scripts#master",
    "standard": "*"
  }
}
