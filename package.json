    "@dooksa/ds-scripts": "bitbucket:dooksa/ds-scripts",
    "standard": "*",
    "@dooksa-extra/ds-plugin-database": "bitbucket:dooksa/ds-plugin-database",
    "@dooksa-extra/ds-plugin-parse": "bitbucket:dooksa/ds-plugin-parse",
    "@dooksa-extra/ds-plugin-template": "bitbucket:dooksa/ds-plugin-template",
    "@dooksa-extra/ds-plugin-utilities": "bitbucket:dooksa/ds-plugin-utilities"
  },
  "dependencies": {
    "@dooksa/resource-loader": "bitbucket:dooksa/resource-loader",
    "@dooksa/ds-plugin": "bitbucket:dooksa/ds-plugin"
  }
