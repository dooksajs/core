{
  "name": "@dooksa/ds-plugin-manager",
  "version": "1.0.0",
  "description": "Plugin manager for Dooksa",
  "main": "./src/index.js",
  "scripts": {
    "lint": "standard",
    "format": "standard --fix",
    "dev": "ds-scripts dev",
