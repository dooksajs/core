{
  "name": "@dooksa/ds-app",
  "version": "1",
  "description": "Dooksa core app",
  "main": "src/index.js",
  "scripts": {
    "lint": "standard",
    "format": "standard --fix",
    "dev": "webpack serve --config webpack.dev.config.js",
    "build": "webpack --config webpack.prod.config.js",
    "build:hash": "npm run build; node build/createHash.js",
    "test:unit": "cypress run --spec cypress/integration/ds-plugin-starter-kit.spec.js",
    "test:unitrecord": "cypress run --record --spec cypress/integration/ds-plugin-starter-kit.spec.js",
    "e2e": "cypress run --spec cypress/integration/ds-plugin-starter-kit-e2e.spec.js",
    "e2e:record": "cypress run --record --spec cypress/integration/ds-plugin-starter-kit-e2e.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:dooksa/ds-app.git"
  },
  "author": {
    "name": "Thomas David",
    "email": "tom@mightysmile.com"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not IE 11",
    "not OperaMini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^8.7.0",
    "html-webpack-plugin": "^4.5.1",
    "standard": "*",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@dooksa/ds-plugin": "bitbucket:dooksa/ds-plugin",
    "@dooksa/ds-plugin-action": "bitbucket:dooksa/ds-plugin-action",
    "@dooksa/ds-plugin-layout": "bitbucket:dooksa/ds-plugin-layout",
    "@dooksa/ds-plugin-element": "bitbucket:dooksa/ds-plugin-element",
    "@dooksa/ds-plugin-content": "bitbucket:dooksa/ds-plugin-content",
    "@dooksa/ds-plugin-operators": "bitbucket:dooksa/ds-plugin-operators",
    "@dooksa/ds-plugin-event": "bitbucket:dooksa/ds-plugin-event",
    "@dooksa/ds-plugin-parameters": "bitbucket:dooksa/ds-plugin-parameters",
    "@dooksa/ds-plugin-utilities": "bitbucket:dooksa/ds-plugin-utilities"
  }
}
