{
  "name": "@dooksa/ds-plugin-starter-kit",
  "version": "1.0.0",
  "description": "This is a starter kit to build plugins for Dooksa",
  "main": "src/index.js",
  "scripts": {
    "dev": "ds-scripts dev",
    "build": "ds-scripts build",
    "test:unit": "ds-scripts --unit test",
    "test:unit:record": "ds-scripts --unit --record test",
    "test:e2e": "ds-scripts --e2e test",
    "test:e2e:record": "ds-scripts --e2e --record test",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:dooksa/ds-plugin-starter-kit.git"
  },
  "author": {
    "name": "Thomas David",
    "email": "tom@mightysmile.com"
  },
  "devDependencies": {
    "@dooksa/ds-plugin": "bitbucket:dooksa/ds-plugin#master",
    "@dooksa/ds-scripts": "bitbucket:dooksa/ds-scripts#master",
    "standard": "*"
  }
}
